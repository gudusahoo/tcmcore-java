class A
{
    public A()
    {
        System.out.println("in A");
    }
}
class B extends A
{
    int b;
    public B(int x)
    {
        b=x;
        System.out.println("in B");
    }
}
class C extends B
{
    public C(int x)
    {
        System.out.println("in C");
    }
 
    public static void main(String args[])
    {
        new A();
        new B(5);
        new C(7);
    }
}

    
