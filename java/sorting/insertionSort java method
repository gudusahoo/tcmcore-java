//code for insertion sort
public void insertionSort()
{
int in,out;
for(out=1;out<nElems;out++)  //out is dividing line
{
double temp=a[out];
in=out;
while(in>0 && a[in-1]>=temp) //untill one is smaller
{
a[in]=a[in-1];              //shift item right
--in;                       //goleft one position
}
a[in]=temp;
}
}

